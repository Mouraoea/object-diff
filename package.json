{
  "name": "object-diff",
  "version": "1.0.0",
  "description": "A high-performance object diffing library for MongoDB-like objects",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "diff",
    "object",
    "mongodb",
    "comparison",
    "database"
  ],
  "author": "Mouraoea",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mouraoea/object-diff.git"
  },
  "bugs": {
    "url": "https://github.com/Mouraoea/object-diff/issues"
  },
  "homepage": "https://github.com/Mouraoea/object-diff#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ]
}
